{
  "name": "jsdoc-commenter",
  "displayName": "jsdoc-commenter",
  "description": "Toggle JSDoc comment on line or selection",
  "version": "0.1.0",
  "eslintConfig": {
    "extends": "hardesty/prettier"
  },
  "prettier": {
    "semi": false
  },
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jsdoc-commenter.toggle"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jsdoc-commenter.toggle",
        "title": "Toggle JSDoc Comment"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.49.0",
    "eslint": "^7.9.0",
    "eslint-config-hardesty": "^3.5.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}
