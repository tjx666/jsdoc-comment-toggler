{
  "name": "jsdoc-comment-toggler",
  "displayName": "jsdoc comment toggler",
  "description": "Toggle JSDoc comment on line or selection",
  "version": "1.0.1",
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "hardesty/prettier"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-non-null-assertion": 0
    }
  },
  "prettier": {
    "semi": false
  },
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jsdoc-comment-toggler.toggle"
  ],
  "main": "./out/src/extension.js",
  "publisher": "zachhardesty",
  "repository": "https://github.com/zachhardesty7/jsdoc-comment-toggler",
  "contributes": {
    "commands": [
      {
        "command": "jsdoc-comment-toggler.toggle",
        "title": "Toggle JSDoc Comment"
      }
    ]
  },
  "scripts": {
    "clean": "rm -rf ./out",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-hardesty": "^3.5.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "source-map-support": "^0.5.19",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}
