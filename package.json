{
  "name": "jsdoc-comment-toggler",
  "displayName": "jsdoc comment toggler",
  "description": "Toggle JSDoc comment on line or selection",
  "version": "1.1.0",
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "hardesty/prettier"
    ],
    "env": {
      "mocha": true,
      "webextensions": true
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "jsdoc/require-param-type": "off",
          "jsdoc/require-returns-type": "off"
        }
      }
    ],
    "rules": {
      "@typescript-eslint/no-extra-semi": "off",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "error",
      "@typescript-eslint/explicit-module-boundary-types": "error",
      "@typescript-eslint/no-non-null-assertion": "error"
    }
  },
  "prettier": {
    "semi": false
  },
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jsdoc-comment-toggler.toggle"
  ],
  "main": "./out/src/extension.js",
  "publisher": "zachhardesty",
  "repository": "https://github.com/zachhardesty7/jsdoc-comment-toggler",
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "jsdoc-comment-toggler.toggle",
        "title": "Toggle JSDoc Comment"
      }
    ]
  },
  "scripts": {
    "clean": "rm -rf ./out",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "pub": "yarn compile; vsce package; vsce publish",
    "test": "yarn compile; node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.1",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-hardesty": "^3.5.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "source-map-support": "^0.5.19",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0"
  }
}
